# Domestika Credentials
DOMESTIKA_SESSION=your_session_cookie_here
DOMESTIKA_CREDENTIALS=your_credentials_here 

# Optional: Custom download path
# DOWNLOAD_PATH=/Users/username/Downloads/courses
# Or use a relative path:
DOWNLOAD_PATH=./my_courses

# Optional: Maximum concurrent downloads (default: 2)
MAX_CONCURRENT_DOWNLOADS=2

# Optional: Enable debug mode to show detailed logs (default: false)
# When enabled, shows additional information like memory usage, ffmpeg commands, file operations, etc.
DEBUG=false
